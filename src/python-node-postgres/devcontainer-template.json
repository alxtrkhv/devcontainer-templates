{
  "id": "python-node-postgres",
  "version": "1.0.0",
  "name": "Python, Node.js & PostgreSQL",
  "description": "Development container with Python, Node.js, PostgreSQL, and modern tooling (mise, uv, just)",
  "documentationURL": "https://github.com/alxtrkhv/devcontainer-templates",
  "platforms": [
    "Python",
    "Node.js",
    "JavaScript",
    "TypeScript",
    "PostgreSQL"
  ],
  "publisher": "Aleksandras Terechovas",
  "keywords": [
    "python",
    "node",
    "nodejs",
    "postgres",
    "postgresql",
    "mise",
    "uv",
    "ubuntu",
    "fullstack",
    "just"
  ],
  "options": {
    "pythonVersion": {
      "type": "string",
      "description": "Python version to install via mise",
      "proposals": [
        "3.14.2",
        "3.13.11",
        "3.12.12",
        "3.11.14",
        "3.10.19"
      ],
      "default": "3.14.2"
    },
    "nodeVersion": {
      "type": "string",
      "description": "Node.js version to install via mise",
      "proposals": [
        "25.2.1",
        "24.12.0",
        "22.21.1",
        "20.19.6"
      ],
      "default": "24.12.0"
    },
    "postgresVersion": {
      "type": "string",
      "description": "PostgreSQL version for the database service",
      "proposals": [
        "18.1",
        "17.7",
        "16.11",
        "15.15",
        "14.20"
      ],
      "default": "18.1"
    },
    "ubuntuVersion": {
      "type": "string",
      "description": "Ubuntu base image version",
      "proposals": [
        "25.10",
        "25.04",
        "24.04"
      ],
      "default": "25.04"
    },
    "packageManager": {
      "type": "string",
      "description": "Node.js package manager to install",
      "proposals": [
        "pnpm",
        "npm",
        "yarn",
        "bun"
      ],
      "default": "pnpm"
    }
  }
}
